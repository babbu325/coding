package bit;

public class flipBits {

	static int fun(int a, int b){
		int res=0;
		int c=1;
		for(int i=0;i<32;i++){
			System.out.println(c + "  "+ res);
			if(((c&a)>0 &&(c&b)>0) || ((c&a)==0&&(c&b)==0)) res++;
			c = c << 1;
		}
		return res;
	}
	public static void main(String[] args) {
		int a=7, b=10;
		System.out.println(fun(a,b));

	}

}
