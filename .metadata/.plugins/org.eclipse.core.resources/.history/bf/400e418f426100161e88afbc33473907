package linkedlist;


class list
{
String s;
list left;
list right;

              list()
              {
            	this.left=null;
            	this.right=null;
               }
}


public class linkedlist1 {

	public static void main(String[] args) {
		list l1=new list();
		list l2=new list();
		list l3=new list();
		list l4=new list();
		l1.s="my";
		l2.s="name";
		l3.s="is";
		l4.s="Ram";
		
		l1.right=l2;
		l2.right=l3;
		l3.right=l4;
     
		while(l1!=null)
		{
			System.out.printf("%s",l1.s);
			l1=l1.right;
		}
	}

}
