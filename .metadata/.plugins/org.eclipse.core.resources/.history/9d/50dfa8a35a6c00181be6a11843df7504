package graph;

public class cycleInDirectedgraph {

	static Graph g;
	static void addedge(Graph g, int s, int d){
		g.adjlist[s].add(d);
	}
	public static void main(String[] args) {
     g = new Graph(4);
     addedge(g,0,1);
     addedge(g,0,2);
     addedge(g,1,2);
     addedge(g,2,0);
     addedge(g,2,3);
     addedge(g,3,3);	
	}

}
